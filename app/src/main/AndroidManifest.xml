<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.example.um2.aigle.alo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Common.Registration.RegistrationActivity" android:screenOrientation="portrait"/>
        <activity android:name=".Common.Welcome.WelcomeActivity" android:screenOrientation="portrait"/>
        <activity android:name=".Common.Configuration.ConfiguratoinActivity" android:screenOrientation="portrait"></activity>
    </application>

</manifest>